<div class="container">
    <h1>
        <p>Welcome to Wcdonalds!</p>
    </h1>
    <panel>
        <h2>Take your order here</h2>
        <form class="create-form">
            <div class="form-group">

                <input type="text" id="newburger" name="burger">
                <button type="submit" id="addBurger">Add Burger</button>
            </div>
        </form>
    </panel>
    <div class="row">
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="lefthead"><strong>Uneaten Burgers</strong></div>
                <div class="panel-body">
                    <ul>
                       
                        <span><strong> </strong></span>
                        <button type="button" class="devourBurger btn btn-warning" id="eatbutton">Devour</button>
                        <hr>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="righthead"><strong>Devoured Burgers</strong></div>
                <div class="panel-body">
                    <ul>
                        <span><strong></strong></span>
                        <button type="button" class="deleteBurger btn btn-danger" id="trashbutton">Delete</button>
                        <hr>
                    </ul>
                </div>
            </div>
        </div>
    </div>

</div>
